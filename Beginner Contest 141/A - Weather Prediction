#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define all(a) a.begin(),a.end()
#define i64 long long int
int main()
{
    string s;
    cin>>s;
    if(s == "Sunny") pf("Cloudy");
    else if(s == "Cloudy") pf("Rainy");
    else pf("Sunny");
return 0;
}
