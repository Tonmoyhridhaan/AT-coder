#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define all(a) a.begin(),a.end()
#define i64 long long int
const double pi = 2*acos(0) ;
const int maxn = (i64)2e5+5;
 
int main()
{
    int n,k,q,i,a;
    cin>>n>>k>>q;
    int arr[n+2] = {0};
    for(i = 0 ; i<q ; i++)
    {
        cin>>a;
        arr[a]++;
    }
    for(i = 1 ; i<=n ; i++)
    {
        if(((arr[i]+k)-q)>0) pf("Yes\n");
        else pf("No\n");
    }
return 0;
}
