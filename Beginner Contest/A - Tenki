#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define all(a) a.begin(),a.end()
#define i64 long long int
const double pi = 2*acos(0) ;
const int maxn = (i64)2e5+5;
 
int main()
{
    string s1,s2;
    cin>>s1>>s2;
    int i ,sum = 0;
    for(i = 0 ; i<3 ; i++)
    {
        if(s1[i] == s2[i]) sum++;
    }
    cout<<sum;
 
 
return 0;
}
